
  <h1><%= @team.name %></h1>
  <h4>Team Motto: <%= @team.motto %></h4>

<h1>Heros</h1>
<% @members.each do |member| %>
  <div class="member">
    <h2>Hero Name: <%= member.name %></h2><br>
    <p>Hero Power: <%= member.power %></p><br>
    <p>Hero Biography: <%= member.bio %></p><br>
  </div><br>
<% end %>



